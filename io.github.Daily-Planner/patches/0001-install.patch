index 9ebac87..895e97c 100644
--- a/OrganiserMVC.pro
+++ b/OrganiserMVC.pro
@@ -59,3 +59,14 @@ FORMS    += views/mainwindow.ui \
 
 RESOURCES += \
     resources.qrc
